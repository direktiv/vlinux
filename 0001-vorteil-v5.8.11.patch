diff --git a/drivers/_vorteil/vlogfs.c b/drivers/_vorteil/vlogfs.c
new file mode 100644
index 000000000..de6103470
--- /dev/null
+++ b/drivers/_vorteil/vlogfs.c
@@ -0,0 +1,277 @@
+/*
+ * Resizable simple ram filesystem for Linux.
+ *
+ * Copyright (C) 2000 Linus Torvalds.
+ *               2000 Transmeta Corp.
+ *
+ * Usage limits added by David Gibson, Linuxcare Australia.
+ * This file is released under the GPL.
+ */
+ /*
+  *  Simple filesystem for log file redirection in vorteil.
+  *  It is basically a copy to replace only file and dir operations
+  */
+#include <linux/fs.h>
+#include <linux/pagemap.h>
+#include <linux/highmem.h>
+#include <linux/time.h>
+#include <linux/init.h>
+#include <linux/string.h>
+#include <linux/backing-dev.h>
+#include <linux/ramfs.h>
+#include <linux/sched.h>
+#include <linux/parser.h>
+#include <linux/magic.h>
+#include <linux/slab.h>
+#include <linux/uaccess.h>
+#include <linux/vmalloc.h>
+#include <linux/fsnotify.h>
+#include "internal.h"
+
+struct vlogfs_mount_opts {
+	umode_t mode;
+};
+
+struct vlogfs_fs_info {
+	struct vlogfs_mount_opts mount_opts;
+};
+
+#define RAMFS_DEFAULT_MODE	0755
+
+static const struct super_operations vlogfs_ops;
+static const struct inode_operations vlogfs_dir_inode_operations;
+static int vwrite_end(struct file *f, struct address_space *mapping, loff_t pos, unsigned len, unsigned copied, struct page *page, void *fsdata);
+
+static const struct address_space_operations vlogfs_aops = {
+	.readpage	= simple_readpage,
+	.write_begin	= simple_write_begin,
+	.write_end	= vwrite_end,
+	.set_page_dirty	= __set_page_dirty_no_writeback,
+};
+
+static int vwrite_end(struct file *f, struct address_space *mapping, loff_t pos, unsigned len, unsigned copied, struct page *page, void *fsdata) {
+
+	fsnotify_modify(f);
+	return simple_write_end(f, mapping, pos, len, copied, page, fsdata);
+
+}
+
+struct inode *vlogfs_get_inode(struct super_block *sb,
+				const struct inode *dir, umode_t mode, dev_t dev)
+{
+	struct inode * inode = new_inode(sb);
+
+	if (inode) {
+		inode->i_ino = get_next_ino();
+		inode_init_owner(inode, dir, mode);
+		inode->i_mapping->a_ops = &vlogfs_aops;
+		mapping_set_gfp_mask(inode->i_mapping, GFP_HIGHUSER);
+		mapping_set_unevictable(inode->i_mapping);
+		inode->i_atime = inode->i_mtime = inode->i_ctime = current_time(inode);
+		switch (mode & S_IFMT) {
+		default:
+			init_special_inode(inode, mode, dev);
+			break;
+		case S_IFREG:
+			inode->i_op = &vlogfs_file_inode_operations;
+			inode->i_fop = &vlogfs_file_operations;
+			break;
+		case S_IFDIR:
+			inode->i_op = &vlogfs_dir_inode_operations;
+			inode->i_fop = &simple_dir_operations;
+
+			/* directory inodes start off with i_nlink == 2 (for "." entry) */
+			inc_nlink(inode);
+			break;
+		case S_IFLNK:
+			inode->i_op = &page_symlink_inode_operations;
+			inode_nohighmem(inode);
+			break;
+		}
+	}
+	return inode;
+}
+
+/*
+ * File creation. Allocate an inode, and we're done..
+ */
+/* SMP-safe */
+static int
+vlogfs_mknod(struct inode *dir, struct dentry *dentry, umode_t mode, dev_t dev)
+{
+	struct inode * inode = vlogfs_get_inode(dir->i_sb, dir, mode, dev);
+	int error = -ENOSPC;
+
+	if (inode) {
+		d_instantiate(dentry, inode);
+		dget(dentry);	/* Extra count - pin the dentry in core */
+		error = 0;
+		dir->i_mtime = dir->i_ctime = current_time(dir);
+	}
+	return error;
+}
+
+static int vlogfs_mkdir(struct inode * dir, struct dentry * dentry, umode_t mode)
+{
+	int retval = vlogfs_mknod(dir, dentry, mode | S_IFDIR, 0);
+	if (!retval)
+		inc_nlink(dir);
+	return retval;
+}
+
+static int vlogfs_create(struct inode *dir, struct dentry *dentry, umode_t mode, bool excl)
+{
+	return vlogfs_mknod(dir, dentry, mode | S_IFREG, 0);
+}
+
+static int vlogfs_symlink(struct inode * dir, struct dentry *dentry, const char * symname)
+{
+	struct inode *inode;
+	int error = -ENOSPC;
+
+	inode = vlogfs_get_inode(dir->i_sb, dir, S_IFLNK|S_IRWXUGO, 0);
+	if (inode) {
+		int l = strlen(symname)+1;
+		error = page_symlink(inode, symname, l);
+		if (!error) {
+			d_instantiate(dentry, inode);
+			dget(dentry);
+			dir->i_mtime = dir->i_ctime = current_time(dir);
+		} else
+			iput(inode);
+	}
+	return error;
+}
+
+static const struct inode_operations vlogfs_dir_inode_operations = {
+	.create		= vlogfs_create,
+	.lookup		= simple_lookup,
+	.link		= simple_link,
+	.unlink		= simple_unlink,
+	.symlink	= vlogfs_symlink,
+	.mkdir		= vlogfs_mkdir,
+	.rmdir		= simple_rmdir,
+	.mknod		= vlogfs_mknod,
+	.rename		= simple_rename,
+};
+
+/*
+ * Display the mount options in /proc/mounts.
+ */
+static int vlogfs_show_options(struct seq_file *m, struct dentry *root)
+{
+	struct vlogfs_fs_info *fsi = root->d_sb->s_fs_info;
+
+	if (fsi->mount_opts.mode != RAMFS_DEFAULT_MODE)
+		seq_printf(m, ",mode=%o", fsi->mount_opts.mode);
+	return 0;
+}
+
+static const struct super_operations vlogfs_ops = {
+	.statfs		= simple_statfs,
+	.drop_inode	= generic_delete_inode,
+	.show_options	= vlogfs_show_options,
+};
+
+enum {
+	Opt_mode,
+	Opt_err
+};
+
+static const match_table_t tokens = {
+	{Opt_mode, "mode=%o"},
+	{Opt_err, NULL}
+};
+
+static int vlogfs_parse_options(char *data, struct vlogfs_mount_opts *opts)
+{
+	substring_t args[MAX_OPT_ARGS];
+	int option;
+	int token;
+	char *p;
+
+	opts->mode = RAMFS_DEFAULT_MODE;
+
+	while ((p = strsep(&data, ",")) != NULL) {
+		if (!*p)
+			continue;
+
+		token = match_token(p, tokens, args);
+		switch (token) {
+		case Opt_mode:
+			if (match_octal(&args[0], &option))
+				return -EINVAL;
+			opts->mode = option & S_IALLUGO;
+			break;
+		/*
+		 * We might like to report bad mount options here;
+		 * but traditionally vlogfs has ignored all mount options,
+		 * and as it is used as a !CONFIG_SHMEM simple substitute
+		 * for tmpfs, better continue to ignore other mount options.
+		 */
+		}
+	}
+
+	return 0;
+}
+
+int vlogfs_fill_super(struct super_block *sb, void *data, int silent)
+{
+	struct vlogfs_fs_info *fsi;
+	struct inode *inode;
+	int err;
+
+	fsi = kzalloc(sizeof(struct vlogfs_fs_info), GFP_KERNEL);
+	sb->s_fs_info = fsi;
+	if (!fsi)
+		return -ENOMEM;
+
+	err = vlogfs_parse_options(data, &fsi->mount_opts);
+	if (err)
+		return err;
+
+	sb->s_maxbytes		= MAX_LFS_FILESIZE;
+	sb->s_blocksize		= PAGE_SIZE;
+	sb->s_blocksize_bits	= PAGE_SHIFT;
+	sb->s_magic		= RAMFS_MAGIC;
+	sb->s_op		= &vlogfs_ops;
+	sb->s_time_gran		= 1;
+
+	inode = vlogfs_get_inode(sb, NULL, S_IFDIR | fsi->mount_opts.mode, 0);
+	sb->s_root = d_make_root(inode);
+	if (!sb->s_root)
+		return -ENOMEM;
+
+	return 0;
+}
+
+struct dentry *vlogfs_mount(struct file_system_type *fs_type,
+	int flags, const char *dev_name, void *data)
+{
+	return mount_nodev(fs_type, flags, data, vlogfs_fill_super);
+}
+
+static void vlogfs_kill_sb(struct super_block *sb)
+{
+	kfree(sb->s_fs_info);
+	kill_litter_super(sb);
+}
+
+static struct file_system_type vlogfs_fs_type = {
+	.name		= "vlogfs",
+	.mount		= vlogfs_mount,
+	.kill_sb	= vlogfs_kill_sb,
+	.fs_flags	= FS_USERNS_MOUNT,
+};
+
+int __init init_vlogfs_fs(void)
+{
+	static unsigned long once;
+
+	if (test_and_set_bit(0, &once))
+		return 0;
+
+	return register_filesystem(&vlogfs_fs_type);
+}
+
+fs_initcall(init_vlogfs_fs);
diff --git a/drivers/_vorteil/vlogfs_ops.c b/drivers/_vorteil/vlogfs_ops.c
new file mode 100644
index 000000000..d196008b3
--- /dev/null
+++ b/drivers/_vorteil/vlogfs_ops.c
@@ -0,0 +1,171 @@
+/* file-mmu.c: vlogfs MMU-based file operations
+ *
+ * Resizable simple ram filesystem for Linux.
+ *
+ * Copyright (C) 2000 Linus Torvalds.
+ *               2000 Transmeta Corp.
+ *
+ * Usage limits added by David Gibson, Linuxcare Australia.
+ * This file is released under the GPL.
+ */
+
+/*
+ * NOTE! This filesystem is probably most useful
+ * not as a real filesystem, but as an example of
+ * how virtual filesystems can be written.
+ *
+ * It doesn't get much simpler than this. Consider
+ * that this file implements the full semantics of
+ * a POSIX-compliant read-write filesystem.
+ *
+ * Note in particular how the filesystem does not
+ * need to implement any data structures of its own
+ * to keep track of the virtual data: using the VFS
+ * caches is sufficient.
+ */
+
+
+#include <linux/fs.h>
+#include <linux/mm.h>
+#include <linux/ramfs.h>
+#include <linux/sched.h>
+#include <linux/uio.h>
+#include <linux/circ_buf.h>
+#include <linux/vmalloc.h>
+
+#include "internal.h"
+
+#define VLOG_BUF_SIZE 2097152
+#define circ_count_to_end(circ)                                                \
+	(CIRC_CNT_TO_END((circ)->head, (circ)->tail, VLOG_BUF_SIZE))
+#define circ_space_to_end(circ)                                                \
+	(CIRC_SPACE_TO_END((circ)->head, (circ)->tail, VLOG_BUF_SIZE))
+
+static ssize_t vlogfs_write(struct file *file, const char __user *buffer,
+			    size_t count, loff_t *ppos);
+static ssize_t vlogfs_read(struct file *f, char __user *to, size_t count,
+			   loff_t *ppos);
+static int vlogfs_open(struct inode *inode, struct file *file);
+static int vlogfs_unlink(struct inode *inode, struct dentry *dentry);
+
+struct vlogfs_item {
+	struct circ_buf cbuf;
+	struct spinlock lock;
+};
+
+static unsigned long vlogfs_mmu_get_unmapped_area(struct file *file,
+						  unsigned long addr,
+						  unsigned long len,
+						  unsigned long pgoff,
+						  unsigned long flags)
+{
+	return current->mm->get_unmapped_area(file, addr, len, pgoff, flags);
+}
+
+static ssize_t vlogfs_read(struct file *f, char __user *to, size_t count,
+			   loff_t *ppos)
+{
+	struct vlogfs_item *item;
+	struct inode *inode;
+	struct circ_buf *cbuf;
+	int n;
+
+	inode = f->f_mapping->host;
+	item = inode->i_private;
+
+	spin_lock(&item->lock);
+	cbuf = &item->cbuf;
+
+	n = min(count, (size_t)circ_count_to_end(cbuf));
+	if (n == 0) {
+		goto done;
+	}
+
+	copy_to_user(to, &cbuf->buf[cbuf->tail], n);
+	smp_store_release(&cbuf->tail, (cbuf->tail + n) & (VLOG_BUF_SIZE - 1));
+
+done:;
+	spin_unlock(&item->lock);
+	return n;
+}
+
+static int vlogfs_open(struct inode *inode, struct file *file)
+{
+	struct vlogfs_item *item;
+
+	if (inode->i_private == NULL) {
+		item = vmalloc(sizeof(struct vlogfs_item));
+		item->cbuf.buf = vmalloc(VLOG_BUF_SIZE);
+		item->cbuf.tail = 0;
+		item->cbuf.head = 0;
+
+		spin_lock_init(&item->lock);
+		inode->i_private = item;
+	}
+
+	return 0;
+}
+
+static ssize_t vlogfs_write(struct file *file, const char __user *buffer,
+			    size_t count, loff_t *ppos)
+{
+	struct inode *inode;
+	struct vlogfs_item *item;
+	struct circ_buf *cbuf;
+	off_t g;
+	int n;
+	char *fptr;
+
+	inode = file->f_mapping->host;
+
+	g = i_size_read(inode);
+	file_update_time(file);
+	i_size_write(inode, g + count);
+	mark_inode_dirty(inode);
+
+	item = inode->i_private;
+	spin_lock(&item->lock);
+	cbuf = &item->cbuf;
+
+	// max to write
+	n = min(count, (size_t)circ_space_to_end(cbuf));
+	if (n == 0) {
+		goto done;
+	}
+
+	fptr = &cbuf->buf[cbuf->head];
+	copy_from_user(fptr, buffer, n);
+	smp_store_release(&cbuf->head, (cbuf->head + n) & (VLOG_BUF_SIZE - 1));
+
+done:;
+	spin_unlock(&item->lock);
+	return n;
+}
+
+static int vlogfs_unlink(struct inode *inode, struct dentry *dentry)
+{
+	struct vlogfs_item *item;
+	item = inode->i_private;
+
+	vfree(item->cbuf.buf);
+	vfree(item);
+
+	inode->i_private = NULL;
+	return 0;
+}
+
+const struct file_operations vlogfs_file_operations = {
+	.write = vlogfs_write,
+	.read = vlogfs_read,
+	.mmap = generic_file_mmap,
+	.fsync = noop_fsync,
+	.llseek = generic_file_llseek,
+	.get_unmapped_area = vlogfs_mmu_get_unmapped_area,
+	.open = vlogfs_open,
+};
+
+const struct inode_operations vlogfs_file_inode_operations = {
+	.setattr = simple_setattr,
+	.getattr = simple_getattr,
+	.unlink = vlogfs_unlink,
+    };
