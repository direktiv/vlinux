index 000000000..462453b07
--- /dev/null
+++ b/drivers/_vorteil/vorteilfs.c
@@ -0,0 +1,404 @@
+// SPDX-License-Identifier: GPL-2.0
+/*
+ * Performance events callchain code, extracted from core.c:
+ *
+ *  Copyright (C) 2019 vorteil.io Pty Ltd <support@vorteil.io>
+ */
+#include <linux/fs.h>
+#include <linux/pagemap.h>
+#include <linux/namei.h>
+#include <linux/syscalls.h>
+#include <linux/mutex.h>
+#include <linux/vmalloc.h>
+#include <linux/namei.h>
+
+#include "internal.h"
+
+#define VORTEIL_MAGIC 0xC0FFEE50CC5
+#define VORTEIL_PATH "/vorteil"
+#define VBLOCK_SIZE 512
+
+struct path pathParent;
+
+static struct dentry *vorteilfs_mount(struct file_system_type *fs_type,
+				      int flags, const char *dev_name,
+				      void *data);
+static int vorteilfs_fill_super(struct super_block *sb, void *data, int silent);
+static void vorteilfs_kill_sb(struct super_block *sb);
+static struct inode *vorteilfs_get_inode(struct super_block *sb, umode_t mode);
+static int vorteilfs_mknode(char *parent, char *name, umode_t mode,
+			    struct inode **inode);
+static int vorteilfs_readpage(struct file *file, struct page *page);
+ssize_t vorteilfs_read(struct file *f, char __user *b, size_t s, loff_t *o);
+static int vorteilfs_release(struct inode *inode, struct file *file);
+static int vorteilfs_open(struct inode *inode, struct file *file);
+
+struct star_header { /* byte offset */
+	char name[100]; /*   0 */
+	char mode[8]; /* 100 */
+	char uid[8]; /* 108 */
+	char gid[8]; /* 116 */
+	char size[12]; /* 124 */
+	char mtime[12]; /* 136 */
+	char chksum[8]; /* 148 */
+	char typeflag; /* 156 */
+	char linkname[100]; /* 157 */
+	char magic[6]; /* 257 */
+	char version[2]; /* 263 */
+	char uname[32]; /* 265 */
+	char gname[32]; /* 297 */
+	char devmajor[8]; /* 329 */
+	char devminor[8]; /* 337 */
+	char prefix[131]; /* 345 */
+	char atime[12]; /* 476 */
+	char ctime[12]; /* 488 */
+	/* 500 */
+};
+
+struct file_details {
+	struct mutex fmtx;
+};
+
+struct inode_details {
+	size_t disk_size;
+	void *file_mem;
+	off_t disk_offset;
+	struct mutex imtx;
+	atomic_t c;
+};
+
+static struct file_system_type vorteilfs_fs_type = {
+	.name = "vorteilfs",
+	.mount = vorteilfs_mount,
+	.kill_sb = vorteilfs_kill_sb,
+	.fs_flags = FS_USERNS_MOUNT,
+};
+
+static const struct super_operations vorteilfs_ops = {
+	.statfs = simple_statfs,
+	.drop_inode = generic_delete_inode,
+};
+
+const struct file_operations vorteilfs_file_operations = {
+	.mmap = generic_file_readonly_mmap,
+	.llseek = generic_file_llseek,
+	.read = vorteilfs_read,
+	.open = vorteilfs_open,
+	.release = vorteilfs_release,
+};
+
+static const struct address_space_operations vorteil_aops = {
+	.readpage = vorteilfs_readpage
+};
+
+const struct inode_operations vorteilfs_file_inode_operations = {
+	.setattr = simple_setattr,
+	.getattr = simple_getattr,
+};
+
+static const struct inode_operations vorteilfs_dir_inode_operations = {
+	.lookup = simple_lookup,
+};
+
+static int vorteilfs_release(struct inode *inode, struct file *file)
+{
+	struct inode_details *idet;
+	idet = inode->i_private;
+
+	mutex_lock(&idet->imtx);
+	atomic_dec(&idet->c);
+	if (!atomic_read(&idet->c)) {
+		vfree(idet->file_mem);
+		idet->file_mem = NULL;
+		vfree(file->private_data);
+		file->private_data = NULL;
+	}
+	mutex_unlock(&idet->imtx);
+
+	return 0;
+}
+
+static int vorteilfs_open(struct inode *inode, struct file *file)
+{
+	struct inode_details *idet;
+	struct file_details *fdet;
+	struct file *f;
+	loff_t o;
+
+	idet = inode->i_private;
+
+	mutex_lock(&idet->imtx);
+	if (idet->file_mem == NULL) {
+		idet->file_mem = vmalloc(idet->disk_size);
+		if (IS_ERR(idet->file_mem)) {
+			panic("can not allocate memory (vorteilfs)");
+		}
+		f = filp_open(vorteil_disk(), O_RDONLY, 0);
+		if (f == NULL) {
+			panic("can not read disk (vorteilfs): %s",
+			      vorteil_disk());
+		}
+		o = idet->disk_offset;
+		kernel_read(f, idet->file_mem, inode->i_size, &o);
+		filp_close(f, 0);
+	}
+
+	fdet = vmalloc(sizeof(struct file_details));
+	memset(fdet, 0, sizeof(struct file_details));
+	mutex_init(&fdet->fmtx);
+	file->private_data = fdet;
+
+	atomic_inc(&idet->c);
+	mutex_unlock(&idet->imtx);
+	return 0;
+}
+
+static int vorteilfs_readpage(struct file *file, struct page *page)
+{
+	struct file_details *fdet;
+	struct inode_details *idet;
+	struct inode *inode;
+	loff_t offset, size;
+	unsigned long fillsize;
+	void *buf;
+	int ret;
+
+	inode = page->mapping->host;
+	idet = inode->i_private;
+	fdet = file->private_data;
+
+	mutex_lock(&fdet->fmtx);
+
+	buf = kmap(page);
+	if (!buf) {
+		ret = -ENOMEM;
+		goto done;
+	}
+
+	offset = page_offset(page);
+	size = i_size_read(inode);
+	fillsize = 0;
+	ret = 0;
+	if (offset < size) {
+		size -= offset;
+		fillsize = size > PAGE_SIZE ? PAGE_SIZE : size;
+		memcpy(buf, idet->file_mem + offset, fillsize);
+		if (ret < 0) {
+			SetPageError(page);
+			fillsize = 0;
+			ret = -EIO;
+		}
+	}
+
+	if (fillsize < PAGE_SIZE)
+		memset(buf + fillsize, 0, PAGE_SIZE - fillsize);
+
+	if (ret == 0)
+		SetPageUptodate(page);
+
+	flush_dcache_page(page);
+	kunmap(page);
+	unlock_page(page);
+
+done:;
+	mutex_unlock(&fdet->fmtx);
+	return ret;
+}
+
+ssize_t vorteilfs_read(struct file *f, char __user *to, size_t count,
+		       loff_t *ppos)
+{
+	struct file_details *fdet;
+	struct inode_details *idet;
+
+	struct inode *inode;
+	ssize_t ret;
+
+	fdet = f->private_data;
+	inode = f->f_inode;
+	idet = inode->i_private;
+
+	mutex_lock(&fdet->fmtx);
+
+	if (fdet == NULL || idet == NULL) {
+		panic("vorteilfs_read: NULL\n");
+	}
+
+	ret = simple_read_from_buffer(to, count, ppos, idet->file_mem,
+				      inode->i_size);
+
+	mutex_unlock(&fdet->fmtx);
+
+	return ret;
+}
+
+static int vorteilfs_fill_super(struct super_block *sb, void *data, int silent)
+{
+	struct inode *inode;
+
+	sb->s_maxbytes = MAX_LFS_FILESIZE;
+	sb->s_blocksize = PAGE_SIZE;
+	sb->s_blocksize_bits = PAGE_SHIFT;
+	sb->s_magic = VORTEIL_MAGIC;
+	sb->s_op = &vorteilfs_ops;
+	inode = vorteilfs_get_inode(sb, S_IFDIR | 0644);
+	sb->s_root = d_make_root(inode);
+	if (!sb->s_root)
+		return -ENOMEM;
+
+	return 0;
+}
+
+static struct inode *vorteilfs_get_inode(struct super_block *sb, umode_t mode)
+{
+	struct inode *inode = new_inode(sb);
+
+	if (inode) {
+		inode->i_ino = get_next_ino();
+		mapping_set_gfp_mask(inode->i_mapping, GFP_HIGHUSER);
+		mapping_set_unevictable(inode->i_mapping);
+		inode->i_atime = inode->i_mtime = inode->i_ctime =
+			current_time(inode);
+		inode->i_mode = mode;
+
+		switch (mode & S_IFMT) {
+		default:
+			init_special_inode(inode, mode, 0);
+			break;
+		case S_IFREG:
+			inode->i_op = &vorteilfs_file_inode_operations;
+			inode->i_fop = &vorteilfs_file_operations;
+			inode->i_data.a_ops = &vorteil_aops;
+			break;
+		case S_IFDIR:
+			inode->i_op = &vorteilfs_dir_inode_operations;
+			inode->i_fop = &simple_dir_operations;
+			inc_nlink(inode);
+			break;
+		case S_IFLNK:
+			inode->i_op = &page_symlink_inode_operations;
+			inode_nohighmem(inode);
+			break;
+		}
+	}
+	return inode;
+}
+
+static int vorteilfs_mknode(char *parent, char *name, umode_t mode,
+			    struct inode **inode)
+{
+	struct dentry *dentry;
+
+	sb_start_write(pathParent.dentry->d_sb);
+	inode_lock(pathParent.dentry->d_inode);
+
+	dentry = lookup_one_len(name, pathParent.dentry, strlen(name));
+	if (IS_ERR(dentry)) {
+		return -EINVAL;
+	}
+
+	*inode = vorteilfs_get_inode(dentry->d_sb, mode);
+	d_add(dentry, *inode);
+
+	inode_unlock(pathParent.dentry->d_inode);
+	sb_end_write(pathParent.dentry->d_sb);
+
+	return 0;
+}
+
+static void vorteilfs_kill_sb(struct super_block *sb)
+{
+	kill_litter_super(sb);
+}
+
+int vorteilfs_create_files(void)
+{
+	char *ep, b[VBLOCK_SIZE];
+	int size, rsize, err;
+	loff_t o;
+	struct inode *inode;
+	struct star_header *sh;
+	struct inode_details *det;
+	struct file *f;
+
+	printk(KERN_INFO "creating vorteil files from %s\n",
+	       (char *)vorteil_disk());
+
+	f = filp_open(vorteil_disk(), O_RDONLY, 0);
+	if (f == NULL) {
+		panic("can not read disk");
+	}
+
+	// go to first tar offset, 66 lbas in
+	o = (66 * VBLOCK_SIZE);
+	do {
+		kernel_read(f, b, VBLOCK_SIZE, &o);
+
+		sh = (struct star_header *)b;
+		if (strncmp(sh->magic, "ustar", strlen("ustar"))) {
+			break;
+		}
+
+		size = simple_strtol(sh->size, &ep, 8);
+		rsize = ((size + VBLOCK_SIZE - 1) / VBLOCK_SIZE) * VBLOCK_SIZE;
+
+		if ((err = vorteilfs_mknode(VORTEIL_PATH, sh->name,
+					    S_IFREG | 0750, &inode))) {
+			return err;
+		} else {
+			inode->i_size = size;
+			det = vmalloc(sizeof(struct inode_details));
+			memset(det, 0, sizeof(struct inode_details));
+			det->disk_offset = o;
+			det->disk_size = rsize;
+			inode->i_private = det;
+			mutex_init(&det->imtx);
+		}
+
+		o += rsize;
+
+	} while (1);
+
+	filp_close(f, 0);
+
+	return 0;
+}
+
+static struct dentry *vorteilfs_mount(struct file_system_type *fs_type,
+				      int flags, const char *dev_name,
+				      void *data)
+{
+	struct dentry *d;
+	d = mount_nodev(fs_type, flags, data, vorteilfs_fill_super);
+	if (IS_ERR(d)) {
+		panic("can not mount vorteilfs (init)");
+	}
+
+	return d;
+}
+
+int __init init_vorteilfs_fs(void)
+{
+	return register_filesystem(&vorteilfs_fs_type);
+}
+
+
+
+void vorteil_setup_fs(void)
+{
+	struct inode *inode;
+
+	if (do_mount("none", "/vorteil", "vorteilfs", 0, 0)) {
+		panic("could not mount vorteilfs");
+	}
+
+	kern_path("/vorteil", LOOKUP_FOLLOW, &pathParent);
+	inode = pathParent.dentry->d_inode;
+	inode->i_mode = S_IFREG | 0755;
+
+	if (vorteilfs_create_files()) {
+		panic("could not create file in vorteilfs");
+	}
+}
+
+fs_initcall(init_vorteilfs_fs);
diff --git a/drivers/_vorteil/vtty.c b/drivers/_vorteil/vtty.c
new file mode 100644
index 000000000..af082f97c
--- /dev/null
+++ b/drivers/_vorteil/vtty.c
@@ -0,0 +1,158 @@
+#include <linux/init.h>
+#include <linux/kernel.h>
+#include <linux/cdev.h>
+#include <linux/fs.h>
+#include <linux/tty.h>
+#include <linux/ktime.h>
+#include <linux/fsnotify.h>
+
+#define MSG_MAGIC_NUMBER 0x21
+#define MSG_IOCTL_OUTPUT _IOW(MSG_MAGIC_NUMBER, 1, int)
+#define MSG_IOCTL_OUTPUT_STYLE _IOW(MSG_MAGIC_NUMBER, 2, int)
+
+#define CHECK_BIT(var, pos) ((var) & (1 << (pos)))
+
+struct cdev cdev;
+
+struct vtty_in {
+	int mode, tty, ttyS0, ttyRedir;
+};
+
+struct vtty_data {
+	struct file *tty;
+	struct file *ttyS0;
+	struct file *ttyRedir;
+};
+
+struct vtty_data dev = { 0 };
+
+static int vtty_open(struct inode *inode, struct file *file)
+{
+	return 0;
+}
+
+static ssize_t vtty_write(struct file *file, const char __user *user_buffer,
+			  size_t size, loff_t *offset)
+{
+	int len;
+
+	if (dev.tty != NULL) {
+		len = (dev.tty->f_op->write)(dev.tty, user_buffer, size,
+					     offset);
+	}
+
+	if (dev.ttyS0 != NULL) {
+		len = (dev.ttyS0->f_op->write)(dev.ttyS0, user_buffer, size,
+					       offset);
+	}
+
+	if (dev.ttyRedir != NULL) {
+		len = (dev.ttyRedir->f_op->write)(dev.ttyRedir, user_buffer,
+						  size, offset);
+		if (len > 0) {
+			fsnotify_modify(dev.ttyRedir);
+		}
+	}
+
+	*offset += len;
+	return len;
+}
+
+static long vtty_ioctl(struct file *file, unsigned int cmd, unsigned long arg)
+{
+	int in, wr;
+
+	switch (cmd) {
+	case MSG_IOCTL_OUTPUT:
+		if (copy_from_user(&in, (int *)arg, sizeof(int)))
+			return -EFAULT;
+
+		// 4 is just an "addon". No previous setting is getting changed
+		if (in < 4) {
+			if (dev.ttyS0) {
+				filp_close(dev.ttyS0, NULL);
+			}
+
+			if (dev.tty) {
+				filp_close(dev.tty, NULL);
+			}
+		}
+
+		printk(KERN_INFO "vtty ioctl value: %d\n", in);
+
+		// standard 0
+		// screen 1
+		// serial 2
+		// disabled 3
+		// redirect 4 (independent from other settings)
+		switch (in) {
+		case 4: {
+			if (dev.ttyRedir) {
+				filp_close(dev.ttyRedir, NULL);
+			}
+			dev.ttyRedir =
+				filp_open("/vlogs/stdout",
+					  O_RDWR | O_CREAT | O_APPEND, 0644);
+			break;
+		}
+		case 3: {
+			dev.tty = NULL;
+			dev.ttyS0 = NULL;
+			break;
+		}
+		case 2: {
+			dev.tty = NULL;
+			dev.ttyS0 = filp_open("/dev/ttyS0", O_RDWR, 0);
+			break;
+		}
+		case 1: {
+			dev.tty = filp_open("/dev/tty1", O_RDWR, 0);
+			dev.ttyS0 = NULL;
+			break;
+		}
+		default: {
+			dev.tty = filp_open("/dev/tty1", O_RDWR, 0);
+			dev.ttyS0 = filp_open("/dev/ttyS0", O_RDWR, 0);
+		}
+		}
+		break;
+	default:
+		return -ENOTTY;
+	}
+
+	// check if ttyS0 can receive data
+	// if I can not write we disable it
+	if (dev.ttyS0 != NULL) {
+		loff_t offset = 0;
+		wr = kernel_write(dev.ttyS0, "", 1, &offset);
+		printk(KERN_INFO "check serial write: %d\n", wr);
+		if (wr != 1) {
+			dev.ttyS0 = NULL;
+		}
+	}
+
+	return 0;
+}
+
+const struct file_operations vtty_fops = { .owner = THIS_MODULE,
+					   .open = vtty_open,
+					   .write = vtty_write,
+					   .compat_ioctl = vtty_ioctl,
+					   .unlocked_ioctl = vtty_ioctl };
+
+int __init init_vtty(void)
+{
+	struct class *vtty_class;
+	dev_t dev;
+
+	alloc_chrdev_region(&dev, 0, 0, "vtty_driver");
+	vtty_class = class_create(THIS_MODULE, "vtty_class");
+	cdev_init(&cdev, &vtty_fops);
+	dev = MKDEV(MAJOR(dev), MINOR(dev));
+	device_create(vtty_class, NULL, dev, NULL, "vtty");
+	cdev_add(&cdev, dev, 1);
+
+	return 0;
+}
+
+device_initcall(init_vtty);
