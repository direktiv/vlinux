index 000000000..b0408a136
--- /dev/null
+++ b/drivers/_vorteil/bootdev.c
@@ -0,0 +1,51 @@
+// SPDX-License-Identifier: GPL-2.0-only
+/*
+ * Simple /proc file to pass bootdevice name to vorteil vinitd
+ *
+ * (C) 2020 vorteil.io pty ltd
+ */
+#include <linux/proc_fs.h>
+
+#include "internal.h"
+
+static ssize_t bootdev_read(struct file *file, char __user *buf, size_t count,
+			    loff_t *ppos);
+
+static struct proc_ops bootdevops = {
+	.proc_read = bootdev_read,
+};
+
+static ssize_t bootdev_read(struct file *file, char __user *buf, size_t count,
+			    loff_t *ppos)
+{
+	char *disk = vorteil_disk();
+	int ml = strlen(disk);
+
+	if (*ppos >= ml) {
+		return 0;
+	}
+
+	if (count > ml) {
+		count = ml;
+	}
+
+	*ppos = count;
+
+	copy_to_user(buf, disk, count);
+
+	return count;
+}
+
+int __init init_boot_dev_info(void)
+{
+	struct proc_dir_entry *proc_boot_dev;
+
+	proc_boot_dev = proc_create("bootdev", 0, NULL, &bootdevops);
+
+	if (proc_boot_dev == NULL)
+		return -ENOMEM;
+
+	return 0;
+}
+
+device_initcall(init_boot_dev_info);
diff --git a/drivers/_vorteil/disk.c b/drivers/_vorteil/disk.c
new file mode 100644
index 000000000..750c4d82c
--- /dev/null
+++ b/drivers/_vorteil/disk.c
@@ -0,0 +1,78 @@
+// SPDX-License-Identifier: GPL-2.0-only
+/*
+ * Finds the bootdevice based on partition uuid and sets it as
+ * root device.
+ *
+ * (C) 2020 vorteil.io pty ltd
+ */
+
+#include <linux/init.h>
+#include <linux/kernel.h>
+#include <linux/root_dev.h>
+#include <linux/delay.h>
+#include <linux/device.h>
+#include <linux/genhd.h>
+#include <linux/syscalls.h>
+#include <linux/mount.h>
+
+#define MAX_TRIES 1000
+
+#define VORTEIL_PART_UUID "PARTUUID=4048447D-C09D-D111-B245-5FFDCE74FAD2"
+
+static char disk_base[32], disk_base_root[32];
+
+// returns the root device without partition, e.g. /dev/sda2
+char *vorteil_disk(void)
+{
+	if (strlen(disk_base) == 0) {
+		panic("no vorteil boot drive stored\n");
+	}
+
+	return disk_base;
+}
+
+// returns the root device including partition, e.g. /dev/sda2
+char *vorteil_disk_root(void)
+{
+	if (strlen(disk_base_root) == 0) {
+		panic("no vorteil root drive stored\n");
+	}
+
+	return disk_base_root;
+}
+
+int __init init_disk_info(void)
+{
+
+	struct gendisk *gd;
+	dev_t dev;
+	int part, a;
+
+	for (a = 0; a < MAX_TRIES; a++) {
+		dev = name_to_dev_t(VORTEIL_PART_UUID);
+		gd = get_gendisk(dev, &part);
+
+		if (gd != NULL) {
+
+			sprintf(disk_base_root, "/dev/%s%d", gd->disk_name,
+				part);
+			sprintf(disk_base, "/dev/%s", gd->disk_name);
+
+			printk(KERN_INFO "vorteil disks: %s, %s\n",
+			       disk_base_root, disk_base);
+
+			goto done;
+		}
+
+		msleep(5);
+	}
+
+	if (disk_base_root[0] == 0) {
+		panic("no vorteil boot drive found\n");
+	}
+
+done:;
+	return 0;
+}
+
+late_initcall_sync(init_disk_info);
